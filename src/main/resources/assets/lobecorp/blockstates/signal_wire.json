{
  "multipart": [
    {
      "apply": {
        "model": "lobecorp:block/signal_wire/center"
      },
      "when": {
        "show_center": "true"
      }
    },
    {
      "apply": {
        "model": "lobecorp:block/signal_wire/side"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "lobecorp:block/signal_wire/side",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "lobecorp:block/signal_wire/side",
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "lobecorp:block/signal_wire/side",
        "y": 270
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "lobecorp:block/signal_wire/side",
        "x": 270
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "lobecorp:block/signal_wire/side",
        "uvlock": false,
        "x": 90
      },
      "when": {
        "down": "true"
      }
    }
  ]
}